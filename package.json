{
  "name": "@jupiterone/graph-npm",
  "version": "1.3.0",
  "description": "A graph conversion tool for https://www.npmjs.com/",
  "license": "MPL-2.0",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "j1-integration collect",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "prebuild": "yarn test:ci",
    "build": "tsc -p tsconfig.dist.json --declaration",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@jupiterone/integration-sdk": "^0.17.0",
    "@lifeomic/attempt": "^3.0.0",
    "libnpm": "^3.0.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@pollyjs/adapter-node-http": "^4.0.4",
    "@pollyjs/core": "^4.0.4",
    "@pollyjs/persister-fs": "^4.0.4",
    "@types/har-format": "^1.2.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "wait-for-expect": "^3.0.2"
  }
}
